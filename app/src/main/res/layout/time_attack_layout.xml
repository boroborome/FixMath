                    android:layout_height="wrap_content"
                    android:layout_weight="1" />
            </LinearLayout>

        </GridLayout>
    </RelativeLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/back_line"
        android:layout_alignTop="@+id/t_line1"
        android:layout_alignBottom="@+id/t_line1"
        android:layout_marginBottom="12dp"
        android:scaleType="fitXY"
        android:foregroundGravity="fill_horizontal|top|left|bottom|fill_vertical|right|fill" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:id="@+id/timer_parent_layout">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:id="@+id/timerLayout">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0."
            android:id="@+id/minutes"
            android:layout_alignBottom="@+id/seconds"
            android:layout_toLeftOf="@+id/seconds"
            android:textSize="30sp"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            android:fontFamily="sans-serif-black"
            android:visibility="invisible" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="00"
            android:id="@+id/seconds"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:textSize="25sp"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            android:fontFamily="sans-serif-black"
            android:visibility="invisible" />

        </RelativeLayout>
    </RelativeLayout>

