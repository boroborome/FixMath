            android:textColor="#FFFFFF"
            android:onClick="FigureClick"
            android:layout_toRightOf="@+id/t_symbol2"
            android:visibility="gone"
            android:gravity="center_vertical|center_horizontal" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="47dp"
            android:id="@+id/t_symbol3"
            android:paddingLeft="2dp"
            android:paddingRight="2dp"
            android:textSize="25sp"
            android:textStyle="bold"
            android:layout_alignParentTop="true"
            android:layout_marginTop="5dp"
            android:textColor="#FFFFFF"
            android:layout_toRightOf="@+id/t_var3"
            android:gravity="center_vertical|center_horizontal"
            android:visibility="gone" />

        <TextView
            android:layout_width="47dp"
            android:layout_height="47dp"
            android:id="@+id/t_var4"
            android:layout_marginTop="5dp"
            android:textSize="30sp"
            android:fontFamily="sans-serif-black"
            android:textStyle="bold"
            android:layout_alignParentTop="true"
            android:clickable="true"
            android:textColor="#FFFFFF"
            android:onClick="FigureClick"
            android:layout_toRightOf="@+id/t_symbol3"
            android:visibility="gone"
            android:gravity="center_vertical|center_horizontal" />

    </RelativeLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PAUSED"
        android:id="@+id/pauseText"
        android:visibility="invisible"
        android:textStyle="bold"
        android:textSize="30sp"
        android:textColor="#FFFFFF"
        android:fontFamily="sans-serif-black"
        android:layout_alignTop="@+id/t_line1"
        android:layout_alignParentRight="true"
        android:layout_alignParentLeft="true"
        android:gravity="center_horizontal"
        android:layout_marginTop="10dp" />

    <TextView
        android:id="@+id/scorePopUp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/multiplerView"
        android:layout_alignRight="@+id/multiplerView"
        android:fontFamily="sans-serif-black"
        android:textColor="#000000"
        android:textSize="50sp"
        android:textStyle="bold" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="25dp"
        android:id="@+id/time_attack_back"
        android:src="@drawable/hamburger_03_03"
        android:layout_alignParentRight="false"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="10dp"
        android:onClick="backToChallengeMenu"
        android:visibility="invisible" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:id="@+id/imageView4"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:src="@drawable/logo_s"
        android:layout_marginBottom="5dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3"
        android:id="@+id/counterView"
        android:textColor="#FFFFFF"
        android:textSize="100sp"
        android:gravity="center_vertical|center_horizontal"
        android:layout_centerInParent="true"
        android:textStyle="bold"
        android:fontFamily="sans-serif-black"
        android:visibility="visible" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/complete_timeattack_level"
        android:layout_centerInParent="true"
        android:visibility="invisible">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="200dp"
            android:id="@+id/lvl_complete_window"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_centerInParent="true"
            android:src="@drawable/timeattack_level_complete_window" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="59.000"
            android:id="@+id/your_score"
            android:textStyle="bold"
            android:textSize="17sp"
            android:fontFamily="sans-serif-black"
            android:layout_alignParentRight="false"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="false"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="10dp"
            android:textColor="#FFFFFF" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="59.000"
            android:id="@+id/best_score"
            android:textStyle="bold"
            android:textSize="17sp"
            android:fontFamily="sans-serif-black"
            android:layout_alignParentRight="false"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="false"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="10dp"
            android:textColor="#ffca0a" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignBottom="@+id/lvl_complete_window"
            android:layout_alignTop="@+id/lvl_complete_window"
            android:gravity="bottom|center_horizontal"
            android:foregroundGravity="center_horizontal"
            android:paddingBottom="7dp"
            android:layout_marginBottom="5dp">

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/back_to_challenge_menu"
                android:onClick="backToChallengeMenu"
                android:layout_alignBottom="@+id/lvl_complete_window"
                android:visibility="visible"
                android:src="@drawable/hamburger_03_03"
                android:layout_marginRight="10dp" />

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/leaderboardOpenBtn"
                android:src="@drawable/leaderboard_button"
                android:onClick="leaderboradOpen"/>

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:id="@+id/restar_btn"
                android:layout_alignBottom="@+id/lvl_complete_window"
                android:layout_toRightOf="@+id/back_to_challenge_menu"
                android:layout_marginLeft="10dp"
                android:onClick="restartLevel"
                android:src="@drawable/repeat_level_btn" />
        </LinearLayout>

    </RelativeLayout>


</RelativeLayout>