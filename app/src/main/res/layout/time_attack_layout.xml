        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/t_correct_f3"
            android:layout_alignLeft="@+id/correctFigure_3"
            android:layout_alignTop="@+id/correctFigure_3"
            android:layout_alignRight="@+id/correctFigure_3"
            android:layout_alignBottom="@+id/correctFigure_3"
            android:visibility="gone" />

        <TextView
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:id="@+id/correctFigure_3"
            android:layout_alignParentLeft="false"
            android:layout_alignParentTop="false"
            android:textSize="20sp"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            android:fontFamily="sans-serif-black"
            android:layout_marginLeft="10dp"
            android:gravity="center_vertical|center_horizontal"
            android:layout_toRightOf="@+id/correctFigure_2"
            android:visibility="gone" />

