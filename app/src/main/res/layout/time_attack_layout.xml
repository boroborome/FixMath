<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/time_attack_layout"
    android:background="#000000">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/time_attack_layoutView"
        android:scaleType="fitXY"/>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/time_attack_background"
        android:src="#000000"
        android:scaleType="fitXY"/>

    <ImageView
        android:layout_width="40dp"
        android:layout_height="20dp"
        android:id="@+id/play_pause_btn"
        android:src="@drawable/pause_btn"
        android:layout_alignParentRight="true"
        android:layout_alignParentLeft="false"
        android:onClick="pauseGame"
        android:layout_marginTop="10dp"
        android:visibility="invisible" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Score"
        android:id="@+id/textScoreView"
        android:textSize="15dp"
        android:textStyle="bold"
        android:fontFamily="sans-serif-black"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="false"
        android:layout_above="@+id/scoreView"
        android:layout_marginRight="5dp"
        android:textColor="#FFFFFF"
        android:layout_marginBottom="-8dp"
        android:visibility="invisible" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:id="@+id/scoreView"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginRight="5dp"
        android:textStyle="bold"
        android:fontFamily="sans-serif-black"
        android:textColor="#FFFFFF"
        android:textSize="22dp"
        android:layout_marginBottom="3dp"
        android:visibility="invisible" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/multiplerView"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        android:fontFamily="sans-serif-black"
        android:layout_toLeftOf="@+id/textScoreView"
        android:layout_alignBottom="@+id/textScoreView"
        android:layout_marginRight="3dp"
        android:layout_marginBottom="2dp" />

    <RelativeLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:id="@+id/correctFigures"
        android:layout_marginTop="90dp"
        android:gravity="center_horizontal"
        android:visibility="invisible">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
