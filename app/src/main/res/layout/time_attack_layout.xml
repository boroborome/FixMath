            android:layout_alignLeft="@+id/correctFigure_1"
            android:layout_alignRight="@+id/correctFigure_1"
            android:layout_alignBottom="@+id/correctFigure_1"
            android:layout_alignTop="@+id/correctFigure_1"
            android:visibility="gone" />

        <TextView
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:id="@+id/correctFigure_1"
            android:layout_alignParentLeft="false"
            android:layout_alignParentTop="false"
            android:textSize="20sp"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            android:fontFamily="sans-serif-black"
            android:gravity="center_vertical|center_horizontal"
            android:visibility="gone" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/t_correct_f2"
            android:layout_alignLeft="@+id/correctFigure_2"
            android:layout_alignRight="@+id/correctFigure_2"
